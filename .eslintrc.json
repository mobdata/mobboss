{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "parser": "babel-eslint",
  "globals": {
    "document": true,
    "window": true
  },
  "rules": {
    "semi": [0],
    "no-multi-spaces": [2, { "exceptions": { "VariableDeclarator": true }}],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"]}],
    "react/destructuring-assignment": [0],
    "no-underscore-dangle": "off"
  }
}
